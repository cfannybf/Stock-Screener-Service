<Window x:Class="ScreenerShell.Shell"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ScreenerShell"
        xmlns:prism="http://www.codeplex.com/prism"
        xmlns:inf="clr-namespace:Screener.Infrastructure"
        mc:Ignorable="d"
        Title="Shell">
    <DockPanel LastChildFill="True">
        <ContentControl DockPanel.Dock="Top" prism:RegionManager.RegionName="ToolbarRegion"></ContentControl>
        <ContentControl prism:RegionManager.RegionName="ContentRegion"></ContentControl>
    </DockPanel>
</Window>
